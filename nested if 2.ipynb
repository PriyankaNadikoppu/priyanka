   "source": [
    "#nested if for checking the number is both pos and even\n",
    "num= int(input(\"enter the number\"))\n",
    "if num>0:\n",
    "    print(\"positive number\")\n",
    "    if num%2==0:\n",
    "        print(\"even\")\n",
    "    else:\n",
    "        print(\"odd\")\n",
    "else:\n",
    "    print(\"number is non-positive\")\n"
   ]
